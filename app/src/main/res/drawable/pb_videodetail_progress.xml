<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#50000000" android:endColor="#50000000" android:angle="270.0" android:centerY="0.75" android:centerColor="#50000000" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#ff4edc38" android:endColor="#ff4edc38" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff4edc38" />
            </shape>
        </clip>
    </item>
</layer-list>